class utilsChronvs {
  constructor(tela, libscreen) {
    this.tela = tela;
    this.libscreen = libscreen;
    this.main = {};
  }
  clearScreen() {
    this.tela.clear();
    D4.set();
  }
  setMain(main) {
    this.main = main;
  }
  getMain() {
    return this.main;
  }
  drwStr(valor, x, y) {
    this.tela.drawString(this.tela.wrapString(valor, this.tela.getWidth()).join("\n"), x, y);
  }
  setFontA() {
    this.libscreen.setFontA();
  }
  getHeight() {
    return 135;
  }
}
exports = utilsChronvs